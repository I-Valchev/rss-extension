    "type": "bolt-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "YourNameHere",
            "email": "youremail@acme.com"
        }
    ],
    "require-dev": {
        "bolt/core": "^4.0.0"
    },
    "autoload": {
